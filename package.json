{
    "name": "stackscript",
    "displayName": "STACKSCRIPT",
    "description": "VS Code extension for STACKSCRIPT language",
    "version": "0.0.1",
    "publisher": "STACKSCRIPT-official",
    "repository": {
        "type": "git",
        "url": "https://github.com/replit-user/STACKSCRIPT-vscode"},
    "engines": {
        "vscode": "^1.78.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Linters"
    ],
    "contributes": {
        "languages": [{
            "id": "stackscript",
            "aliases": ["STACKSCRIPT", "stackscript"],
            "extensions": [".stack"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "stackscript",
            "scopeName": "source.stackscript",
            "path": "./syntaxes/stackscript.tmLanguage.json"
        }],
        "snippets": [{
            "language": "stackscript",
            "path": "./snippets/snippets.json"
        }]
    },
    "activationEvents": [
        "*"
    ],
    "main": "./extension.js"
}